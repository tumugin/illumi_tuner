{
  "name": "illumi_tuner",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --mode=development",
    "builddev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "prod": "webpack --mode=production && yarn run copyfunctionfiles",
    "ssr:dev": "NODE_ENV=development ts-node ./ssr-server.ts",
    "ssr:prod": "NODE_ENV=production ts-node ./ssr-server.ts",
    "lint": "eslint --ext .ts,.js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "storybook": "start-storybook -p 9001",
    "buildstorybook": "IS_STORYSHOT=1 build-storybook -o ./storyshot-public",
    "test": "yarn buildstorybook && jest --verbose",
    "gitcleanup": "git remote prune origin && git branch -v | cut -c 3- | awk '$3 ~/\\[gone\\]/ { print $1 }' | xargs git branch --delete",
    "clean:dev": "rm -rf dist dist-server",
    "clean:prod": "rm -rf prod prod-server",
    "clean": "yarn run clean:dev && yarn run clean:prod",
    "deploy": "yarn run clean:prod && yarn run prod && firebase deploy",
    "copyfunctionfiles": "mkdir -p functions/dist/ && cp prod/vue-ssr-client-manifest.json functions/dist/ && cp prod-server/vue-ssr-server-bundle.json functions/dist/ && cp public/index.ssr.template.html functions/dist/"
  },
